
<h1>Listing Tips</h1>

<%= link_to 'Add Tip', new_tip_path %>

<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
  </tr>

  <% @tips.each do |tip| %>
    <tr>
      <td><%= tip.title %></td>
      <td><%= tip.text %></td>
      <td><%= link_to 'Show', tip_path(tip) %></td>
    </tr>
  <% end %>
</table>
